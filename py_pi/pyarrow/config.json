{
  "native_identifier": "pyarrow",
  "repo": "apache/arrow",
  "registry": "py_pi",
  "tag_formats": [
    "apache-arrow-{major}.{minor}.{patch}",
    "apache-arrow-{major}.{minor}.0",
    "apache-arrow-{major}.{minor}.1",
    "apache-arrow-0.{minor}.{patch}",
    "apache-arrow-{major}.{patch}.0"
  ],
  "sentinel_timestamp": "2020-01-01T00:00:00Z",
  "include": [
    "docs/**/*.md",
    "cpp/**/*.cpp",
    "cpp/**/*.hpp",
    "dev/**/*.yml",
    "dev/**/*.md",
    "dev/**/*.sh",
    "python/**/*.md",
    "python/**/*.py",
    "python/**/*.txt",
    "python/**/*.sh"
  ],
  "collection_name_prefix": "pyarrow",
  "version_sample_relative_size": 0.5,
  "version_sample_max_size": 5
}